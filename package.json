{
	"name": "cloudflare-asset-worker",
	"version": "0.0.0",
	"type": "module",
	"description": "A Cloudflare Worker for serving static assets from KV storage",
	"scripts": {
		"dev": "wrangler dev -c ./workers/router/wrangler.jsonc -c ./workers/asset-service/wrangler.jsonc",
		"deploy": "bun run --filter '@cloudflare-asset-worker/*' deploy"
	},
	"workspaces": [
		"workers/*",
		"cli"
	]
}
