{
	"$schema": "../../cli/deploy.config.schema.json",
	"projectName": "TanStack Start App",
	"assets": {
		"directory": "./dist/client",
		"patterns": ["**/*"],
		"ignore": ["**/*.map"]
	},
	"server": {
		"entrypoint": "index.js",
		"modulesDirectory": "./dist/server",
		"compatibilityDate": "2025-09-24"
	},
	"run_worker_first": ["/_serverFn/*"],
	"config": {
		"html_handling": "none",
		"not_found_handling": "none",
		"headers": {
			"rules": {
				"/_build/*": {
					"set": {
						"Cache-Control": "public, max-age=31536000, immutable"
					}
				}
			}
		}
	},
	"env": {
		"ENVIRONMENT": "production"
	}
}
